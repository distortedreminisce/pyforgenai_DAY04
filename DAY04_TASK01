imoort datetime
import random

#random
for i in range (0,5):
	random_int = random.randint(1, 101)
	print(random_int)

j = random.randint(0,6)
color_list = ["red", "orange", "yellow", "green", "blue"]
print(color_list[j])

shuffled_color_list = []
while len(color_list) != 0:
	k = random.randint(0,len(color_list))
	shuffled_color_list.append(color_list[k])
print(shuffled_color_list)


